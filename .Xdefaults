Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true

Xcursor.theme: Jimmac

URxvt.font: -*-profont-*-*-*-*-11-*-*-*-*-*-*-*
URxvt.boldFont: -*-profont-*-*-*-*-11-*-*-*-*-*-*-*
URxvt.italicFont: -*-profont-*-*-*-*-11-*-*-*-*-*-*-*
URxvt.bolditalicFont: -*-profont-*-*-*-*-11-*-*-*-*-*-*-*
urxvt*scrollBar: false
urxvt*depth: 32
urxvt*background: #151515
urxvt*foreground: #E6E6E6
urxvt*cursorColor: #ffffff

URxvt*perl-ext-common: default,matcher
URxvt*matcher.button: 3
URxvt.urlLauncher: firefox
URxvt.urgentOnBell: true

xterm*background: #151515
xterm*foreground: #E6E6E6
xterm*font: -*-profont-*-*-*-*-11-*-*-*-*-*-*-*

